{
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import",
      [
        "module-resolver",
        {
          "root": ["./script/"],
          "alias": {
            "@server/bin": "./script/bin",
            "@server/controllers": "./script/controllers",
            "@server/middlewares": "./script/middlewares",
            "@server/models": "./script/models",
            "@server/public": "./script/public",
            "@server/routes": "./script/routes",
            "@server/services": "./script/services",
            "@server/socket": "./script/socket",
            "@server/views": "./script/views"
          }
        }
      ]
    ]
  }