{
  "name": "expressjs-mvc-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node --inspect ./script/bin/wwwboot.js",
    "build": "yarn build:server && yarn build:client",
    "build:server": "webpack --mode production",
    "build:client": "webpack --config webpack.config.client.js --mode production",
    "webpackStart": "node ./dist/server",
    "sequelize": "./node_modules/.bin/sequelize",
    "build:babel": "babel script -d ./build",
    "start:build:babel": "node ./build/bin/wwwboot.js",
    "initDB": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "createDB": "sequelize db:create",
    "dropDB": "sequelize db:drop",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed",
    "seedAll": "sequelize db:seed:all"
  },
